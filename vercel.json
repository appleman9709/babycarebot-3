{
  "version": 2,
  "builds": [
    {
      "src": "mini_app/app_vercel.py",
      "use": "@vercel/python",
      "config": {
        "requirements": "mini_app/requirements_vercel.txt"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "mini_app/app_vercel.py"
    }
  ],
  "env": {
    "PYTHONPATH": "mini_app"
  }
}
